{
  "name": "Lexica Grabber 2",
  "description": "Extract all images from current web page",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches":["https://lexica.art/*" ],
      "all_frames": true,
      "js":["content-script.js"]
    }
  ],
  "icons": {
    "16": "./icons/icon16.png",
    "19": "./icons/icon19.png",
    "32": "./icons/icon32.png",
    "38": "./icons/icon38.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "action": { },
  "host_permissions":["https://lexica.art/*" ],
  "web_accessible_resources": [
    {
      "resources": ["content-script.css"],
      "matches":["https://lexica.art/*" ]
    }
  ]
}